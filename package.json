{
  "name": "React-Redux-Boilerplate",
  "version": "0.0.1",
  "description": "My own boilerplate",
  "scripts": {
    "start": "npm run dev:start",
    "dev:start": "run-p dev:client:build dev:server:prepare dev:server:build dev:server:delay",
    "dev:server:prepare": "universal-webpack --settings ./webpack/universal-webpack-settings.json prepare",
    "dev:server:build": "webpack --config ./webpack/webpack.config.server.development.babel.js --watch --colors --display-error-details",
    "dev:server:delay": "run-s delay dev:server:start",
    "dev:server:start": "nodemon ./src/server/start.babel.js --watch ./static/server",
    "dev:client:build": "babel-node ./webpack/webpack-dev-server.js",
    "delay": "node ./node_modules/npm-delay 20000",
    "prod:start": "npm-run-all prod:client:build prod:server:build prod:server:start",
    "prod:client:build": "webpack --config ./webpack/webpack.config.client.production.babel.js --colors --display-error-details",
    "prod:server:build": "webpack --config ./webpack/webpack.config.server.production.babel.js --colors --display-error-details",
    "prod:server:start": "better-npm-run prod:server:start"
  },
  "betterScripts": {
    "prod:server:start": {
      "env": {
        "NODE_ENV": "production"
      },
      "command": "node ./src/server/start.babel.js"
    }
  },
  "author": "Nick Cain",
  "keywords": [
    "react",
    "isomorphic",
    "universal",
    "webpack",
    "express",
    "hot reloading",
    "react-hot-reloader",
    "redux",
    "starter",
    "boilerplate",
    "babel",
    "redux-saga"
  ],
  "license": "MIT",
  "dependencies": {
    "babel-polyfill": "^6.20.0",
    "classnames": "^2.2.5",
    "compression": "^1.6.2",
    "express": "^4.14.0",
    "global": "^4.3.2",
    "immutable": "^3.8.1",
    "lodash": "^4.17.4",
    "normalize-scss": "^6.0.0",
    "postcss-svgo": "^2.1.6",
    "prop-types": "^15.5.8",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-form": "^1.2.6",
    "react-helmet": "^5.0.3",
    "react-hot-loader": "^1.3.1",
    "react-redux": "^5.0.1",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.8",
    "redux": "^3.6.0",
    "redux-connect": "^5.0.0",
    "redux-immutablejs": "^0.0.8",
    "redux-saga": "^0.14.8",
    "serve-favicon": "^2.3.2"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^19.0.0",
    "babel-loader": "^7.0.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-flow-strip-types": "^6.21.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-preset-stage-2": "^6.22.0",
    "babel-register": "^6.18.0",
    "better-npm-run": "^0.0.15",
    "clean-webpack-plugin": "^0.1.15",
