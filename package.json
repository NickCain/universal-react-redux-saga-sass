    "eslint-plugin-flowtype": "^2.32.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "2",
    "eslint-plugin-react": "^6.8.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "flow-bin": "^0.44.2",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.24.1",
    "node-sass": "^4.5.2",
    "nodemon": "^1.11.0",
